package javaapplication5_4;

/**
 *
 * @author JAVA
 */
public class JavaApplication5_4 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {

    }

    static int maximo(int t[]) {
        int max = t[0];
        for (int e : t) {
            if (e > max) {
                max = e;

            }

        }
        return max;
    }

}
